#!/usr/local/bin/bash
# Test script for running the workbench.
export UNIEDITOR="@GNUCLIENT@ -eval '(setq stored-name \"%beN\")' \"%bF\""
export UNISERVER=`hostname`
export UNIWORKINGDIR=/tmp/uni.temp.$USER
gmake killServer
gmake killWorkbench
gmake runServer
if test @WORK_AROUND_BDB_LINUX_BUG@ -ne 0
  then
     export MALLOC_CHECK_=1
  fi
gmake runWorkbench
