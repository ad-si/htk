#!/bin/sh
# LaTeX's a file, called from MMiSSLaTeX
#
# Argument 1 = working directory
# Argument 2 = name of TeX file in directory
# We run three times through LaTeX.  Hopefully that will be enough.
cd $1
export TEXINPUTS
TEXINPUTS=@TOP@/mmiss/mmisslatex/:$TEXINPUTS
@LATEX@ $2
@LATEX@ $2
@LATEX@ $2
